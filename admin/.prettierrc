{
  "semi": true,
  "tabWidth": 2,
  "printWidth": 100,

  "useTabs": false,
  "singleQuote": true,
  "insertPragma": false,
  "bracketSpacing": true,
  "requirePragma": false,
  "jsxSingleQuote": false,
  "bracketSameLine": false,
  "vueIndentScriptAndStyle": false,

  "quoteProps": "as-needed",
  "arrowParens": "always",
  "trailingComma": "es5",
  "endOfLine": "lf"
}
